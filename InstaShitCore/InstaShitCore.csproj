<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.0</Version>
    <Authors>Konrad Krawiec</Authors>
    <Company />
    <Description>Core of InstaShit - bot for Insta.Ling which solves daily sessions.</Description>
    <PackageProjectUrl>https://instashit.pl</PackageProjectUrl>
    <RepositoryUrl>https://github.com/konrad11901/InstaShitCore</RepositoryUrl>
    <PackageReleaseNotes>Some structural changes which make InstaShitCore's code cleaner.
You don't need now to specify the base location - InstaShitCore can work without any files.</PackageReleaseNotes>
    <PackageLicenseUrl>https://raw.githubusercontent.com/konrad11901/InstaShitCore/master/LICENSE</PackageLicenseUrl>
    <RepositoryType />
    <PackageTags>InstaLing Insta.Ling</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>7</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
