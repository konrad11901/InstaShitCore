<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.3</Version>
    <Authors>Konrad Krawiec</Authors>
    <Company />
    <Description>Core of InstaShit - bot for Insta.Ling which solves daily sessions.</Description>
    <PackageProjectUrl>https://instashit.pl</PackageProjectUrl>
    <RepositoryUrl>https://github.com/konrad11901/InstaShitCore</RepositoryUrl>
    <PackageReleaseNotes>InstaShit now automatically gets latest version string from Insta.Ling source.</PackageReleaseNotes>
    <PackageLicenseUrl>https://raw.githubusercontent.com/konrad11901/InstaShitCore/master/LICENSE</PackageLicenseUrl>
    <RepositoryType />
    <PackageTags>InstaLing Insta.Ling</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>7</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
